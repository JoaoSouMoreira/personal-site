{
  "root": true,
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "plugins": ["vue"],
  "ignorePatterns": ["node_modules/", ".cache", "dist", "*.spec.*"],
  "rules": {
    "no-console": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  }
}
